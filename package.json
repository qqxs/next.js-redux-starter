        "redux-observable": "^1.2.0",
        "rxjs": "^6.6.3"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^9.5.1",
        "@types/express": "^4.17.9",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/node": "^12.12.53",
        "@types/react": "^16.9.44",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/redux-logger": "^3.0.8",
